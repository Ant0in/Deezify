<children>
    <HBox fx:id="slidersContainer" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
        <children>
        </children>
    </HBox>

    <HBox alignment="CENTER" spacing="10">
        <children>
            <Button fx:id="okButton" text="Ok" />
