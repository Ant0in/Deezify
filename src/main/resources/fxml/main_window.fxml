   <children>
      <Button fx:id="playSongButton" layoutX="274.0" layoutY="57.0" mnemonicParsing="false" onAction="#handlePlaySong" text="▶" />
      <Button fx:id="previousSongButton" layoutX="54.0" layoutY="160.0" mnemonicParsing="false" onAction="#handlePreviousSong" text="⏮" />
      <Button fx:id="nextSongButton" layoutX="509.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleNextSong" text="⏭" />
      <Button fx:id="stopSongButton" layoutX="300.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleStopSong" text="⏹" />
   </children>
</AnchorPane>
