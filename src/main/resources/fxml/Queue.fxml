<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="5" xmlns:fx="http://javafx.com/fxml/1">
    <ListView fx:id="listView" prefWidth="160" styleClass="queue" VBox.vgrow="ALWAYS"/>
    <Button fx:id="addSongButton" maxWidth="Infinity" styleClass="queueButton" text="ADD"/>
    <Button fx:id="deleteSongButton" maxWidth="Infinity" styleClass="queueButton" text="DEL"/>
    <Button fx:id="clearQueueButton" maxWidth="Infinity" styleClass="queueButton" text="CLEAR"/>
</VBox>